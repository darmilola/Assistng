<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:height="20dp" android:width="20dp">
        <shape android:shape="rectangle">
           <corners android:radius="20dp"/>
            <stroke android:color="@color/pink" android:width="3dp"/>
        </shape>
    </item>

    <item android:top="20dp" android:left="9dp" android:width="3dp" android:height="240dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/pink" android:endColor="@color/colorPrimary" android:angle="-90"/>
        </shape>
    </item>

    <item android:top="260dp" android:height="20dp" android:width="20dp">
        <shape android:shape="rectangle">
            <corners android:radius="45dp"/>
            <stroke android:color="@color/colorPrimary" android:width="3dp"/>
        </shape>
    </item>







</layer-list>